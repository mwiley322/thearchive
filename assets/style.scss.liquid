/* =Layout
----------------------------------------------- */

body {
  background: #000;
  color: #fff;
  font-family: 'Open Sans', Arial, Helvetica, sans-serif;
  font-size: 12px;
}

/* =Global
----------------------------------------------- */
/* Headings */
h1, h2, h3, h4, h5, h6 {
  clear: both;
}

h1, h2, h3, h4, h5, p, ul, ol, table, blockquote {
  font-family: 'Open Sans', Arial, Helvetica, sans-serif;
}

hr {
  display: block;
  width: 90%;
  height: 1px;
  border: 0;
  border-top: 3px solid #2C2A29;
  margin: 0 auto;
  padding: 0;
}

/* Text elements */
h1 {
  font-size: 64px;
  letter-spacing: 0.1em;
}

h2 {
  font-size: 36px;
}

.spaced-h2 {
  letter-spacing: 0.15em;
}

h3 {
  font-size: 24px;
}

.spaced-h3 {
  letter-spacing: .15em;
}

h4 {
  font-size: 18px;
  letter-spacing: 0em;
}

h5 {
  font-size: 14px;
  letter-spacing: 0em;
}

p {
  font-size: 12px;
}

ul, ol {
  font-size: 12px;
}

/* Forms */

input[type="text"]::-webkit-input-placeholder,
input[type="password"]::-webkit-input-placeholder,
input[type="email"]::-webkit-input-placeholder {
  color: #9B9B9B;
}

input[type="text"]::-moz-input-placeholder,
 input[type="password"]::-moz-input-placeholder,
 input[type="email"]::-moz-input-placeholder { /* Firefox 19+ */
  color: #9B9B9B;
}
input[type="text"]:-ms-input-placeholder,
input[type="password"]:-ms-input-placeholder,
input[type="email"]:-ms-input-placeholder { /* IE 10+ */
  color: #9B9B9B;
}
input[type="text"]:-moz-input-placeholder,
input[type="password"]:-moz-input-placeholder,
input[type="email"]:-moz-input-placeholder { /* Firefox 18- */
  color: #9B9B9B;
}

input[type=text]:focus:not([readonly]),
input[type=password]:focus:not([readonly]),
input[type=email]:focus:not([readonly]) {
  border-bottom: 1px solid #ccc;
  box-shadow: none;
}

input[type=submit]:hover {
  background-color: #fff;
  -webkit-transition: all 0.5s ease-out;
  -moz-transition: all 0.5s ease-out;
  -o-transition: all 0.5s ease-out;
  transition: all 0.5s ease-out;
}

input[type=number]::-webkit-inner-spin-button,
input[type=number]::-webkit-outer-spin-button {
  -webkit-appearance: none;
  margin: 0;
}

input:-webkit-autofill {
  -webkit-box-shadow: 0 0 0 1000px #000 inset !important;
  -webkit-text-fill-color: #fff;
}

input.hint,
textarea.hint {
  color: #666;
  text-transform: uppercase;
}

img, video, embed, iframe {
  max-width: 100%;
}

/* Links */

a {
  color: #fff;
  text-decoration: none;
  -webkit-transition: all 0.3s ease-in;
  -moz-transition: all 0.3s ease-in;
  -o-transition: all 0.3s ease-in;
  transition: all 0.3s ease-in;
}

a:active, a:focus,
a:hover, li.active a {
  color: #d7cf35;
  text-decoration: none;
}

.side-nav li a:active, .side-nav li a:hover {
  color: #d7cf35;
}


