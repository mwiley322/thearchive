  margin: 50px 0 120px;
  text-align: justify;
  font-size: 12px;
}

.legal-page a {
  color: #d7cf35;
}

.legal-page a:hover {
  color: #fff;
}

.legal-page h2 {
  margin-bottom: 40px;
}


.item {
  padding: 0 .75rem 20px .75rem !important;
}

.image {
  position:relative;
}

.image img {
  width:100%;
  vertical-align:top;
}

.image:after {
  content: attr(data-content);
  position: absolute;
  width: 100%;
  height: 100%;
  top: 0;
  left: 0;
  background: #d7cf35;
  opacity: 0;
  transition: all 0.5s;
  -webkit-transition: all 0.5s;
}

.image:hover:after {
  opacity: .75;
}
